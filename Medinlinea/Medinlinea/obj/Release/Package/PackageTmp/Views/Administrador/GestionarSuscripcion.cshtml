
@{
    ViewBag.Title = "GestionarSuscripcion";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Style/estilos.css" rel="stylesheet" />

<div class="container">
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-10 col-lg-offset-1">

            <form action="" id="form-especialista">
                <h2>Desactivar Suscripcion</h2>
                <br>
                <div class="form-group">
                    <label for="inputEmail3" class="col-sm-3 control-label">Especiaslistas</label>
                    <div class="col-sm-9">
                        <select name="tipo-membresia" name="especialista-consultorio" class="form-control">
                            <option value="premium">Seleccione Especialista</option>
                            <option value="premium">Leonardo Montes</option>
                            <option value="gratuita">David Eduardo</option>
                        </select>
                        <br>
                    </div>
                </div>
            </form>
            <p class="text-center">
                <button type="button" name="paginar" onclick="getPagPublication(null);" class="btn btn-primary btn-sm">Buscar</button>
            </p>

            <br>

            <div class="form-group">
                <label for="inputEmail3" class="col-sm-3 control-label">Estado Suscripcion</label>
                <div class="col-sm-9">
                    <select name="tipo-membresia" name="especialista-consultorio" class="form-control">
                        <option value="premium">Seleccione Estado</option>
                        <option value="premium">Activo</option>
                        <option value="gratuita">Inactivo</option>
                    </select>
                    <br>
                </div>
            </div>
        </div>
    </div>
</div>
