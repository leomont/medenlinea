
@{
    ViewBag.Title = "EliminarArticulos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Style/estilos.css" rel="stylesheet" />

<div class="container">
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-10 col-lg-offset-1">

            <form action="" id="form-especialista">
                <h2 style="text-align: center;">Eliminar Articulos</h2>
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-3 control-label">Articulos</label>
                        <div class="col-sm-9">
                            <select name="tipo-membresia" name="especialidad" class="form-control">
                                <option value="premium">Seleccione Articulo</option>
                                <option value="premium">Articulo Ejemplo 1</option>
                                <option value="gratuita">Articulo Ejemplo 2</option>
                            </select>
                            <br>
                        </div>
                    </div>
            </form>

            <p class="text-center">
                <button type="button" name="paginar" onclick="getPagPublication(null);" class="btn btn-danger btn-sm">Eliminar</button>
            </p>

        </div>
    </div>
</div>

