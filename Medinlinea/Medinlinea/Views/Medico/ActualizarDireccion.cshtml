
@{
    ViewBag.Title = "ActualizarDireccion";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Style/estilos.css" rel="stylesheet" />

<div class="container">
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-10 col-lg-offset-1">

            @*<div class="col-xs-12 col-sm-2">
                <img class="img-responsive" src="../img/manizales/manizales_1.jpg" alt="">
            </div>*@

            <div class="col-xs-12 col-sm-10">

                <form action="" id="form-especialista">
                    <h2 style="text-align: center;">Actualizar Datos Consultorio</h2>
                    @*<section>*@
                        <div class="form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">Departamento</label>
                            <div class="col-sm-9">
                                <select name="tipo-membresia" name="departamento-consultorio" id="departamento-consultorio" class="form-control">
                                    <option value="premium">Seleccione Departamento</option>
                                    <option value="premium">Caldas</option>
                                    <option value="gratuita">Tolima</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">Ciudad</label>
                            <div class="col-sm-9">
                                <select name="tipo-membresia" name="ciudad-consultorio" id="ciudad-consultorio" class="form-control">
                                    <option value="premium">Seleccione Ciudad</option>
                                    <option value="premium">Manizales</option>
                                    <option value="gratuita">Ibague</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">Calle</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" name="calle-consultorio" id="calle-consultorio" placeholder="Ingresa tu nombre">
                                <br>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">telefono</label>
                            <div class="col-sm-9">
                                <input type="tel" class="form-control" name="telefono-consultorio" id="telefono-consultorio" placeholder="Ingresa tu apellido">
                                <br>
                            </div>
                        </div>
                    @*</section>*@
                </form>

            </div>

            <p class="text-center">
                <button type="button" name="paginar" onclick="getPagPublication(null);" class="btn btn-primary btn-sm">Actualizar</button>
            </p>

        </div>
    </div>
</div>


